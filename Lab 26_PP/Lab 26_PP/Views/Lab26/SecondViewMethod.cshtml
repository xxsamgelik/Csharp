@model List<string>

<h2>Отсортированный список овощей (таблица)</h2>

<table class="table" style="width:20%">
    <thead>
        <tr>
            <th class="text-center">Наименование</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var vegetable in Model)
            {
                <tr class="text-center">
                    <td>@vegetable</td>
                </tr>
            }
    </tbody>
</table>
